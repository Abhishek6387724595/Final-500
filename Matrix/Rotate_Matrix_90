#include <bits/stdc++.h> 
#include <unordered_map>
using namespace std; 
#define M 3 
#define N 3 
void printCommonElements(int mat[M][N]) 
{
		for(int i=0;i<3;i++)
        {
            for(int j=2;j>=0;j--)
            {
                cout<<mat[j][i];
            }
            cout<<endl;
        }
}
int main() 
{ 
	int mat[M][N] = 
	{ 
		{1,2,3}, 
		{4,5,6}, 
		{7,8,9} 
	}; 
	printCommonElements(mat);
	return 0; 
}
