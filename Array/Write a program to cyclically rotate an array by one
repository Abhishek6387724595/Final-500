# include <iostream> 
using namespace std; 
  
void rotate(int arr[], int n) 
{ 
    int brr[n];
    int temp = arr[n-1];
    for (int i=0;i<n-1;i++)
    {
        brr[i+1] = arr[i];
    }

    brr[0]=temp;

    for (int i = 0; i < n; i++) 
        cout << brr[i]; 
} 
 
int main()  
{ 
    int arr[] = {1, 2, 3, 4, 5}; 
    int n = sizeof(arr)/sizeof(arr[0]); 
  
    rotate(arr, n); 
} 
